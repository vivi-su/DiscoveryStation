o;\n  }\n}\n\n/* nav dropdown */\n.nav--dropdown{\nfloat:left;\noverflow:hidden;\nmax-width:100%;\n}\n\n.nav--dropdown__content{\nmax-height:0;\noverflow: hidden;\nposition: relative;\nz-index:1;\n-webkit-transition: max-height 0.5s ease-in-out;\n    transition: max-height 0.5s ease-in-out;\n \ncolor:var(--color-primary);\nbackground-color: white;\nmargin-top: 1.5rem;\npadding:0 1rem;\n}\n\n.nav--dropdown__content a{\n \n  display: flex;\n  flex-direction: column;\n padding-left:2rem;\n padding-bottom: 1rem;\n color:var(--color-secondary);\n}\n\n.nav--dropdown:hover .nav--dropdown__content{\n  max-height:80vh;\n}\n\n.dropdown__content__list:hover{\n  color:var(--color-primary);\n}\n\n\n@media screen and (min-width:768px){\n.nav__item {\n   width:auto;\n  }\n\n.nav--dropdown__content{\nmax-height:0;\nposition: absolute;\nborder-radius: 0 0 15px 15px;\n-webkit-box-shadow: 0 10px 6px -6px #777;\n     -moz-box-shadow: 0 10px 6px -6px #777;\n          box-shadow: 0 10px 6px -6px #777;\n}\n\n.nav--dropdown__content a{\n padding-left:0;\n margin-top: 0.5rem;\n}\n\n.nav--dropdown:hover .nav--dropdown__content{\n  border-top:3px solid var(--color-light);\n}\n\n}\n\nhr {\n    border-top: 3px solid #eee;\n    margin: 3rem 0;\n}\n\n/* grid */\n.grid{\n\tdisplay:grid;\n}\n\n@media screen and (min-width:768px){\n\t.grid--1x2{\n\t\tgrid-template-columns:repeat(2,1fr)\n\t}\n}\n\n@media screen and (min-width:1024px){\n\t.grid--1x3{\n\t\tgrid-template-columns:repeat(3,1fr)\n\t}\n}\n\n/* button */\n\n.btn{\n\tborder-radius:20px;\n    border:0;\n\tcursor:pointer;\n\tfont-size:1.8rem;\n\tfont-weight: 600;\n\toutline: 0;\n\tpadding:0.5rem 2rem;\n\ttext-transform: uppercase;\n\ttext-align: center;\n\tmargin: 1rem 0;\n\ttransition: 0.3s;\n}\n\n.btn--light{\n\tbackground: var(--color-light);\n}\n.wrapper{\n\tmax-width:1200px;\n\tmargin:0 auto;\n}\n\n  /* card */\n  .container{\n  padding:2rem;\n  }\n\n  @media screen and (min-width:768px){\n\t.container{\n\t\tpadding:4rem;\n\t\t}\n\t}\n\t@media screen and (min-width:1024px){\n\t\t.container{\n\t\t\tpadding:6rem;\n\t\t\t}\n\t\t}\n\n  .card{\n\t  box-shadow: 0 0 20px 10px #f3f3f3;\n\t  border-radius:20px;\n\t  overflow:hidden;\n\t  margin:2rem;\n  }\n\n  .card--primary .card__header{\n\t  background:var(--color-primary);\n\t  color:#fff;\n\t  font-weight: 600;\n  }\n\n   .card--secondary .card__header{\n\t  background:var(--color-secondary);\n\t  color:#fff;\n\t  font-weight: 600;\n  }\n\n  .card__header,\n  .card__body{\n\t  padding:2rem 3rem;\n  }\n  .card .btn{\n\tmargin:2rem 3rem;\n\t\n  }\n\n  .card  .btn:hover{\n\tmargin:2rem 3rem;\n\tbackground: var(--color-pink);\n  }  \n\n"},"lineCount":null}},"error":null,"hash":"c124d3aea64499a8d8b7b0604a4bd932","cacheData":{"env":{}}}